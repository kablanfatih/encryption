{
  "name": "kablanfatih/encryption",
  "description": "A package for automatically encrypting and decrypting Eloquent attributes in Laravel , based on configuration settings.",
  "keywords": [
    "laravel",
    "encrypt",
    "database",
    "eloquent",
    "model",
    "attribute",
    "decrypt",
    "encryption",
    "decryption",
    "db",
    "aes",
    "laravel encryption",
    "eloquent encryption",
    "model encryption",
    "attribute encryption",
    "database encryption"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Fatih <kablanfatih>",
      "email": "kablanfatih34@gmail.com",
      "homepage": "https://github.com/kablanfatih"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": ">=7.2.0",
    "laravel/framework": "^6.0.0",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5@dev",
    "orchestra/testbench": "4.0"
  },
  "autoload": {
    "psr-4": {
      "Kablanfatih\\Encryption\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Kablanfatih\\Encryption\\": "src/",
      "Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Kablanfatih\\Encryption\\Providers\\EncryptableServiceProvider"
      ]
    }
  }
}

{
    "name": "kablanfatih/encryption",
    "description": "A package for automatically encrypting and decrypting Eloquent attributes in Laravel , based on configuration settings.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "kablanfatih",
            "email": "kablanfatih34@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "~5.6|^7.2"
    },
    "autoload": {
        "psr-4": {
            "Encryption\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Encryption\\": "src/",
            "Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Encryption\\src\\EncryptableServiceProvider"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5@dev"
    }
}
